public:
    Node * removeDuplicates( Node *head) 
    {
    if(head==NULL||head->next==NULL)return head;
     Node *cur=head,*prev=NULL,*next=NULL;
     set<int>s;
     while(cur!=NULL)
     {
         
         if(s.count(cur->data)>0&&cur!=NULL)
         {
            prev->next=cur->next;
            cur=prev->next;
             
         }else{
             s.insert(cur->data);prev=cur;
             cur=cur->next;
         }
     }
     return head;
    }
