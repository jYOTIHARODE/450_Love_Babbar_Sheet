int median(vector<vector<int>> &matrix, int r, int c){
        // code here   
        vector<int>res;
        for(int i=0;i<matrix.size();i++)
        {
            for(int j=0;j<matrix[i].size();j++)
            {
                res.push_back(matrix[i][j]);
            }
        }
        sort(res.begin(),res.end());
        int len=res.size();
        if(len%2==1)
        {
            len=len/2+1;
        }
        else
        len/=2;
        return res[len-1];
        
    }
