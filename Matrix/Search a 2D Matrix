 bool searchMatrix(vector<vector<int>>& matrix, int target) {
        
        
    int target_row=0;
        int c=matrix[0].size();
        for(int i=0;i<matrix.size();i++)
        {
            if(target<=matrix[i][c-1])
            { target_row=i;break;}
                
        }
        for(int j=0;j<matrix[0].size();j++)
        {
            if(matrix[target_row][j]==target)return true;
        }
        return false;
        
    }
